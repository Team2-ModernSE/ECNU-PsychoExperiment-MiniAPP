<!--pages/my/my.wxml-->
<view class="user_info_wrap">
  <view class="user_img_wrap">
    <image class="user_bg" src="{{userinfo.avatarUrl}}"></image>
    <view class="user_info">
      <image class="user_icon" src="{{userinfo.avatarUrl}}"></image>
      <view class="user_name">{{userinfo.nickName}}</view>
    </view>
  </view>
</view>
<view class="user_content">
  <view class="user_main">
    <view class="release">
      <navigator url="/pages/order/released?type=1">
        <view class="release_num">0</view>
        <view class="release_name">我发布的</view>
      </navigator>
    </view>
    <view class="get">
      <navigator url="/pages/order/get?type=1">
        <view class="get_num">0</view>
        <view class="get_name">我领取的</view>
      </navigator>
    </view>
    <view class="name">姓名</view>
    <view class="gender">性别</view>
    <view class="age">年龄</view>
  </view>
  <view>
    <navigator url="../../pages/expInfo2/expInfo2?id=0a4429175fd4cf5a0153bdc26c4a14bc">ExpInfo2入口</navigator>
    <navigator url="../../pages/expInfo3/expInfo3?id=0a4429175fd4cf5a0153bdc26c4a14bc">ExpInfo3入口</navigator>
    <navigator url="../../pages/release/release">实验创建入口</navigator>
    <navigator url="../../pages/register/register">注册入口</navigator>
  </view>
</view>

